<Loja>:
    name: "loja"       
    FloatLayout:
        size: root.width,root.height

        Label:
            text: "Qual tipo de produto você está buscando?"
            size_hint: None, None
            size: (200, 50)
            pos_hint: {"center_x":0.5, "top": .9}
        Spinner:
            id: loja_categoria
            text: 'Categoria'
            values: ['Alimentos','Medicamentos']
            size_hint: 0.8, .2
            pos_hint:{'center_x':0.5, "top":0.7}
        ButtonRounded:
            text: "Voltar"
            size_hint: 0.3,0.1
            on_press: root.voltar()
            size: root.width,root.height
            pos_hint:{'center_x':0.30, "top":0.35}
        ButtonRounded:
            text: "Próximo"
            size_hint: 0.3,0.1
            on_press: root.proximo()
            size: root.width,root.height
            pos_hint:{'center_x':0.7, "top":0.35}
            

<Gerenciador>:
    name: "gerenciador"
    FloatLayout:
        size: root.width,root.height
        ButtonRounded:
            text: "Cadastrar Produto"
            pos_hint: {"x":0.15, "top": 0.55 + 0.3}
            on_release:
                app.root.current = "cadastrarproduto"
                root.manager.transition.direction = "up"

        ButtonRounded:
            text: "Gerenciar Produtos"
            pos_hint: {"x":0.55, "top": 0.55 + 0.3}
            on_release:
                app.root.current = "gerenciarprodutos"
                root.manager.transition.direction = "up"
        
        ButtonRounded:
            text: "Voltar"
            on_release:
                app.root.current = "fornecedor"
                root.manager.transition.direction = "down"
            pos_hint: {"x":0.55, "top": .15 + .3}

<CadastrarProduto>:
    name: "cadastrarproduto"
    BoxLayout:
        orientation: "vertical"
        size: root.width,root.height
        spacing: 5
        padding: 10

        Label:
            size_hint:1,0.1
            text:"*Nome do Produto"
            font_size: 15  
        TextInput:
            id: nome_produto
            hint_text: "Nome do Produto"
            padding: (0,10)
            size_hint: .8,.2
            font_size: 18
        Label:
            size_hint:1,0.1
            text:"*Quantidade do Produto"
            font_size: 15
        TextInput:
            id: quantidade_produto
            hint_text: "Quantidade do Produto"
            size_hint: .8,.2
            font_size: 18
            padding: (0,10)
        Label:
            size_hint:1,0.1
            text:"*PREÇO"
            font_size: 15
        TextInput:
            id: preco_produto
            hint_text: "Informe o preço unitário do produto"
            size_hint: .8,.2
            font_size: 18
            padding: (0,10)
        Label:
            size_hint:1,0.1
            text:"Imagem"
            font_size: 15
        TextInput:
            id: imagem_produto
            hint_text: "Informe o caminho da imagem do produto"
            size_hint: .8,.2
            font_size: 18
            padding: (0,10)
        Label:
            size_hint:1,0.1
            text:"*Descrição"
            font_size: 15
        TextInput:
            id: descricao
            hint_text: "Drescreva o produto"
            size_hint: .8,.2
            font_size: 18
            padding: (0,10)
        Label:
            id: mensagem_produto
            size_hint:1,0.1
            text:"Imagem"
            font_size: 15
        FloatLayout:

            ButtonRounded:
                text: "Cadastrar"
                pos_hint: {"center_x":0.30, "top":0.7}
                on_press: root.cadastro_produto()
            ButtonRounded:
                text: "Voltar"
                pos_hint: {"center_x":0.7,"top":0.7}
                on_release: 
                    app.root.current = "gerenciador"
                    root.manager.transition.direction = "down"
        

