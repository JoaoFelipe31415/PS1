<Login>:
    name: "login"
    
    BoxLayout:
        orientation: "vertical"
        size: root.width,root.height
        spacing: 10
        padding: 5

        Label:
            size_hint: (1,.2)
            text: "Projeto 1"

        TextInput:
            id: usuario
            hint_text: "Usuário"
        TextInput:
            id: senha
            hint_text: "Senha"
            password: True


        FloatLayout:
            ButtonRounded:
                text: "Cadastro"
                on_release: 
                    app.root.current = "cadastro"
                    root.manager.transition.direction = "up"
                pos_hint: {"x":0.18, "top":1}
                

            ButtonRounded:
                text: "Entrar"
                pos_hint: {"x":0.51, "top":1}
                on_press: root.entrar()
            
            ButtonRounded:
                text: "Esqueceu"
                pos_hint:{'x': 0.35, "top": 0.5}
                on_release: 
                    app.root.current = "esqueceu"
                    root.manager.transition.direction = "up"     
        
        FloatLayout:
            Label:
                text: "Marque aqui para Fornecedor"
                size_hint: (None, None)
                size: self.texture_size
                pos_hint: {"center_x": 0.4, "center_y": 0.5}

            CheckBox:
                id: box
                size_hint: (None, None)
                size: (50, 50)
                pos_hint: {"center_x": 0.6, "center_y": 0.5}
                                
        Label:
            id: login_aviso
       
<Cliente>:
