<TextInput>
    multiline: False
    halign: 'center'
    padding_y: 20
    size_hint : .8,0.5
    font_size: 22
    #size: 600,100
    pos_hint:{'center_x':0.5}



<ButtonRounded@Button>
    background_normal: ""
    background_color: (0,0,0,0)
    size_hint: (.3,.3)   
    canvas.before:
        Color:
            rgba:(48/255,84/255,150/255,1)
        RoundedRectangle:
            size: self.size
            pos: self.pos
            radius: [10]
<Deposito>:
    name: "deposito"
    FloatLayout:
        size: root.width,root.height
        padding: 10
        
        Label:  
            id: nome_usuario_fornecedor
            size_hint: 1, None
            size: (500, 50)
            text_size: self.size
            pos_hint: {"x":0.1, "top": 1}
            text: "Fornecedor"
        Label:
            id: data_fornecedor
            size_hint: 1, None
            size: (500, 50)
            text_size: self.size
            pos_hint: {"x":.745, "top": 1}
            text: "Data: 31/08/2024"
        Label:
            id: saldo_fornecedor
            size_hint: 1, None
            size: (500, 50)
            text_size: self.size
            pos_hint: {"x":0.42, "top": 1}
            text: "Saldo: R$ 0,00"
        Label:
            text: "Informe o valor à ser depositado"
            size_hint: 1, None
            size: (500, 50)
            pos_hint: {"x":0, "top": 0.8}
        
        TextInput:
            id: valor_deposito
            pos_hint: {"x":0.3, "top": 0.7}
            size_hint : .4,0.11
            hint_text: "Valor"
        
        ButtonRounded:
            id: botao_depositar
            pos_hint: {"x":0.15, "top": 0.55}
            text: "Depositar"
            size_hint: (0.3,0.1)
            on_press: root.depositar()
        
        ButtonRounded:
            id: voltar_depositar
            pos_hint: {"x":0.55, "top": 0.55}
            text: "Voltar"
            size_hint: (0.3,0.1)
            on_release: 
                app.root.current = "fornecedor"
                root.manager.transition.direction = "down"



<Cadastro>:
    name:"cadastro"
    BoxLayout:
        orientation: "vertical"
        size: root.width,root.height
        spacing: 20
        padding: 10
        
        Label:
            text: "Escolha a forma que deseja interagir com a plataforma."
    
        FloatLayout:
            ButtonRounded:
                text: "Cliente"
                on_release: 
                    app.root.current = "cadastro_cliente"
                    root.manager.transition.direction = "up"
                pos_hint: {"x":0.18, "top":1}
                

            ButtonRounded:
                text: "Fornecedor"
                pos_hint: {"x":0.51, "top":1}
                on_release: 
                    app.root.current = "cadastro_fornecedor"
                    root.manager.transition.direction = "up"
        
            ButtonRounded:
                text: "Voltar"
                pos_hint: {"x":0.34, "top":0}
                on_release: 
                    app.root.current = "login"
                    root.manager.transition.direction = "down"
        Label:
            


    

<Cadastro_Fornecedor>:
    name: "cadastro_fornecedor"
    BoxLayout:
        orientation: "vertical"
        size: root.width,root.height
        spacing: 5
        padding: 10

        Label:
            size_hint:1,0.1
            text:"Nome da empresa"
            font_size: 15  
        TextInput:
            id: empresa_cadastro
            hint_text: "Nome da Empresa"
            padding: (0,10)
            size_hint: .8,.2
            font_size: 18
        Label:
            size_hint:1,0.1
            text:"Senha"
            font_size: 15
        TextInput:
            id: empresa_senha_cadastro
            hint_text: "Crie sua senha"
            size_hint: .8,.2
            font_size: 18
            padding: (0,10)
        Label:
            size_hint:1,0.1
            text:"Categoria"
            font_size: 15
        Spinner:
            id: categoria_cadastro
            text: 'Categoria'
            values: ['Alimentos','Medicamentos']
            size_hint: 0.8, .2
            pos_hint:{'center_x':0.5}
        Label:
            size_hint:1,0.1
            text:"CNPJ"
            font_size: 15
        TextInput:
            id: cnpj_cadastro
            hint_text: "Digite o CNPJ"
            size_hint: .8,.2
            font_size: 18
            padding: (0,10)
        Label:
            size_hint:1,0.1
            text:"Email"
            font_size: 15
        TextInput:
            id: email_cadastro_fornecedor
            hint_text: "Digite o email"
            size_hint: .8,.2
            font_size: 18
            padding: (0,10)
        Label:
            size_hint:1,0.1
        FloatLayout:
            ButtonRounded:
                text: "Cadastro"
                pos_hint: {"center_x":0.35, "top":1}
                on_press: root.apertou()
            ButtonRounded:
                text: "Voltar"
                pos_hint: {"x":0.52, "top":1}
                on_release: 
                    app.root.current = "cadastro"
                    root.manager.transition.direction = "down"
            Label:
                id: label_fornecedor
            
    
