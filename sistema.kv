            size_hint: None, None
            size: (100, 50)
            pos_hint: {"x":.09 + .35, "top": .37}
        
        TextInput:
            id: preco_painel
            size_hint: .2,.1
            hint_text: "Novo Preço"
            pos_hint: {"x":0.06+0.35, "top": .3}
        
        
        

        ButtonRounded:
            text: "Alterar"
            on_press: root.alterar()
            size_hint: (.1,.1)
            pos_hint: {"x":.1+.18, "top": .15}   
        
        Label:
            id:aviso_painel
            size_hint: None, None
            size: (200, 50)
            pos_hint: {"center_x":0.5, "top": .2}  
        
        ButtonRounded:
            text: "Voltar"
            size_hint: (.1,.1)   
            on_release:
                app.root.current = "gerenciador"
                root.manager.transition.direction = "down"
            pos_hint: {"x":.8, "top": .15}
    

<CadastroPet>:
    name: "cadastropet"
    FloatLayout:
        size: root.width,root.height
        BoxLayout:
            orientation: "vertical"
            size: root.width,root.height
            spacing: 5
            padding: 10

            Label:
                size_hint:1,0.1
                text:"Nome do Pet"
                font_size: 15
            TextInput:
                id: pet_cadastro
                hint_text: "Nome do Pet"
                padding: (0,10)
                size_hint: .8,.2
                font_size: 18
            Spinner:
                id: pet_seletor
                text: "Tipo de Pet"
                values: ['Cachorro','Gato']
                size_hint: 0.5, .25
                pos_hint:{'center_x':0.5, "top": 0.9}
                on_text: root.mostrar_tipos(self.text)
                
            Spinner:
                id: raca_seletor
                text: "Raça"
                #values: ['Golden Retrivier','Husky Siberiano']
                size_hint: 0.5, .25
                pos_hint:{'center_x':0.5, "top": 0.9}
                on_text: root.verifica()
                opacity: 0
                
            Label:
                size_hint:1,0.1
            FloatLayout:
                ButtonRounded:
                    text: "Cadastro"
                    pos_hint: {"center_x":0.35, "top":1}
                    on_press: root.apertou()

                ButtonRounded:
                    text: "Voltar"
                    pos_hint: {"x":0.52, "top":1}
                    on_release: 
                        app.root.current = "cliente"
                        root.manager.transition.direction = "down"
                
                Label:
                    id: aviso_pet

<GuiaPet>:
    name: "guiapet"
    FloatLayout:
        size: root.width,root.height
        
        Label:
            text: "Guia Pet"
            size_hint: None, None
            size: (200, 50)
            pos_hint: {"center_x":0.5, "top": 1}  
        
        
        Spinner:
            id: pet_seletor
            text: "Selecione seu Pet"
            #values: ['Cachorro','Gato']
            size_hint: 0.5, .1
            pos_hint:{'center_x':0.5, "top": 0.9}
            on_text: root.mostrar_dicas(self.text)


        Label:
            id: dica
            text_size: self.size
            size_hint: (None, None)
            size: 600, 150
            pos_hint:{'center_x':0.5, "top": 0.8}
        
        ButtonRounded:
            text: "Voltar"
            size_hint: (.1,.1)   
            on_release:
                app.root.current = "cliente"
                root.manager.transition.direction = "down"
            pos_hint: {"center_x":.5, "top": .3}

