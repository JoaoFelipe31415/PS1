<Painel>:
    name: "painel"
    FloatLayout:
        size: root.width,root.height
        Label: 
            id: empresa_painel
            text: 'Bem vindo a loja da empresa'
            size_hint: None, None
            size: (200, 50)
            pos_hint: {'center_x':0.5, "top":1}
        Spinner:
            id: seletor_painel
            text: "Produtos"
            #values: ['Alimentos','Medicamentos']
            size_hint: 0.5, .1
            pos_hint:{'center_x':0.5, "top": 0.9}
            on_text: root.mostrar_produto(self.text)
        Image:
            id: imagem_painel
            opacity: 0
            allow_stretch: True
            keep_ratio: True
            size_hint: (0.4, 0.4)  
            pos_hint: {'center_x': 0.5, 'top': 0.75}
        Label:
            id:mostrar_painel
            size_hint: None, None
            size: (200, 50)
            pos_hint: {"x":0, "top": .5}  
        
        Label:
            text: "Quantidade"
            size_hint: None, None
            size: (100, 50)
            pos_hint: {"x":.09, "top": .4}
        
        TextInput:
            id: quantidade_painel
            size_hint: .2,.1
            hint_text: "Quantidade"
            pos_hint: {"x":0.06, "top": .3}
        
        Label:
            id: saldo_painel
            size_hint: None, None
            size: (200, 50)
            pos_hint: {"center_x":.1, "top": 1}  
        

        ButtonRounded:
            text: "Comprar"
            on_press: root.comprar()
            size_hint: (.1,.1)
            pos_hint: {"x":.1, "top": .15}   
        
        Label:
            id:aviso_painel
            size_hint: None, None
            size: (200, 50)
            pos_hint: {"center_x":0.5, "top": .2}  
        
        ButtonRounded:
            text: "Voltar"
            size_hint: (.1,.1)   
            on_release:
                app.root.current = root.retorno()
                root.manager.transition.direction = "down"
            pos_hint: {"x":.8, "top": .15}

